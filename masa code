import 'calculate.dart';
import 'async_program.dart';

void main() async{                  //main function

  Calculate calcu = Calculate();    //instance  object class

  //data type   object     agroment
    int add = calcu.addition(5,5);
      int sub = calcu.sub(13,6);
        int multi = calcu.multiply(2,5);
          double div = calcu.divide(16,8);
            print("The result of addition is: $add");           //string interpolation
             print("The result of subtraction is: $sub");       //string interpolation
              print("The result of multiplication is: $multi"); //string interpolation
                print("The result of division is: $div");       //string interpolation

    AsyncProgram ap= AsyncProgram();
    ap.displayUsers();
}
